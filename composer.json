{
    "name": "nuclear/nuclear",
    "type": "project",
    "description": "Polymorphic and multilingual CMS powered by Laravel.",
    "keywords": [
        "nuclear",
        "nuclearcms",
        "hierarchy",
        "polymorphic",
        "multilingual",
        "reactor",
        "cms",
        "content management",
        "laravel"
    ],
    "version": "4.0.8",
    "authors": [
        {
            "name": "Egemen Kızılcan",
            "email": "egemen@umomega.com",
            "homepage": "https://umomega.com",
            "role": "Developer / Designer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.5",
        "nuclear/reactor": "^1.0",
        "spatie/laravel-cookie-consent": "^2.12"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --provider=\"Nuclear\\Reactor\\Providers\\ReactorServiceProvider\" --tag=public --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --provider=\"Nuclear\\Reactor\\Providers\\ReactorServiceProvider\" --tag=public --force"
        ]
    }
}
